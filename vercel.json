{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install"
      }
    },
    {
      "src": "src/pages/api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/pages/api/**",
      "use": "@vercel/node"
    }
  ]
}
