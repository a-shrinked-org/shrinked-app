{
  "functions": {
    "src/pages/api/route.py": {
      "maxDuration": 60
    },
  "app/api/capsule/**": {
    "maxDuration": 60
  }
},
  "rewrites": [
    {
      "source": "/api/download",
      "destination": "/app/api/download/route.py"
    },
    {
      "source": "/api/download/(.*)",
      "destination": "/app/api/download/route.py"
    }
  ]
}